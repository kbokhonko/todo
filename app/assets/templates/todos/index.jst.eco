<div class='todo' name='todo'>
    <input id="toggle" type="checkbox" <%= if @todo.get('done') then 'checked'%>/>
    <div class="title"><%= @todo.get('content')%></div> <a class="destroy"></a>
    <a class="title" href='#' visible='hidden'></a> 
    <input class='title_edit hidden' value='<%= @todo.get('content')%>'>
</div>

